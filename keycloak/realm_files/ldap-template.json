{
  "id": "ldap-ucams-id",
  "name": "ucams",
  "providerId": "ldap",
  "config": {
    "connectionUrl": ["LDAP_URL_REPLACEMENT"],
    "usersDn": ["USERSDN_REPLACEMENT"],
    "authType": ["none"],
    "importEnabled": ["true"],
    "editMode": ["READ_ONLY"],
    "vendor": ["other"]
  },
  "subComponents": {
    "org.keycloak.storage.ldap.mappers.LDAPStorageMapper": {
      "username": {
        "id": "mapper-username-id",
        "name": "username",
        "providerId": "user-attribute-ldap-mapper",
        "config": {
          "ldap.attribute": ["uid"],
          "read.only": ["true"],
          "always.read.value.from.ldap": ["false"],
          "user.model.attribute": ["username"]
        }
      },
      "email": {
        "id": "mapper-email-id",
        "name": "email",
        "providerId": "user-attribute-ldap-mapper",
        "config": {
          "ldap.attribute": ["mail"],
          "read.only": ["true"],
          "always.read.value.from.ldap": ["false"],
          "user.model.attribute": ["email"]
        }
      }
    }
  }
}
